        "xunit": "2.2.0",
        "dotnet-test-xunit": "2.2.0-preview2-build1029"
      }
    },
    "net45": {
      "dependencies": {
        "xunit": "2.2.0"
      },
      "frameworkAssemblies": {
        "System.Runtime": {
          "type": "build"
