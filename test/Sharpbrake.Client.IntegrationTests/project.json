  "copyright": "Copyright (c) 2017 Airbrake Technologies, Inc.",
  "version": "1.0.0-*",

  "dependencies": {
    "Sharpbrake.Client": {
      "target": "project"
    },
    "Newtonsoft.Json.Schema": "2.0.7"
  },

  "buildOptions": {
    "copyToOutput": "notice_schema.json"
  },

  "frameworks": {
    "netcoreapp1.0": {
      "dependencies": {
        "Microsoft.NETCore.App": {
          "type": "platform",
          "version": "1.0.1"
